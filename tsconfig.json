{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/layout.js", "src/app/page.js", "src/app/(search)/search/page.js", "src/app/(prepay)/prepay/page.js", "src/app/(faq)/faq/page.js", "src/app/(contact)/contact/page.js", "src/app/(about)/about/page.js", "src/components/asks/AskContainer.js", "src/components/Header/HeaderDesktop.js", "src/components/Search/FirstSlide.js", "src/components/Search/SecondSlide.js", "src/components/userInfo/UserInfo.js"  ],
  "exclude": [
    "node_modules"
  ]
}
